module tb_half_adder;

        reg a,b;
        wire sum, carry;
        
        half_adder A1(.a(a),.b(b),.sum(sum),.carry(carry));
        
            initial begin
            #5 a = 0;
               b = 0;
            
            #5 a = 0;
               b = 1;
               
            #5 a = 1;
               b = 0;    
               
            #5 a = 1;
               b = 1;    
            
            end     
endmodule
